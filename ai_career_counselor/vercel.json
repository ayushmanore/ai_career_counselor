{
  "version": 2,
  "builds": [
    {
      "src": "ai_career_counselor/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "your_project_name/wsgi.py"
    }
  ]
}